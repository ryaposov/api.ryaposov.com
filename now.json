{
  "version": 2,
  "env": {
    "DB_URI": "@ryaposov_api_db_uri",
    "AUTH_SECRET": "@ryaposov_api_auth_secret",
    "TELEGRAM_API_KEY": "@ryaposov_api_telegram_api_key",
    "TELEGRAM_CHAT_ID": "@ryaposov_api_telegram_chat_id"
  },
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
